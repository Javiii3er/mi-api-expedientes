{
  "name": "api-expedientes-indicios",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "lint": "eslint .",
    "format": "prettier --write .",
    "db:up": "cross-env sqlcmd -S %DB_SERVER%,%DB_PORT% -d %DB_NAME% -U %DB_USER% -P %DB_PASSWORD% -i scripts/schema.sql",
    "db:seed": "cross-env sqlcmd -S %DB_SERVER%,%DB_PORT% -d %DB_NAME% -U %DB_USER% -P %DB_PASSWORD% -i scripts/seed.sql"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@types/react-bootstrap": "^1.0.1",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^5.3.8",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "mssql": "^10.0.4",
    "react-bootstrap": "^2.10.10",
    "react-hook-form": "^7.62.0",
    "react-router-dom": "^7.8.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/bootstrap": "^5.2.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/express-rate-limit": "^5.1.3",
    "@types/helmet": "^0.0.48",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/mssql": "^9.1.7",
    "@types/node": "^20.19.11",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "cross-env": "^10.0.0",
    "eslint": "^9.6.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "prettier": "^3.2.5",
    "tsx": "^4.16.2",
    "typescript": "^5.4.5"
  }
}
